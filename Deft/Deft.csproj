<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>1.0.2</Version>
    <Authors>Worly</Authors>
    <Company>Canvas</Company>
    <Product>DeftNetworking</Product>
    <PackageId>DeftNetworking</PackageId>
    <RepositoryUrl>https://github.com/Worly/deft-networking</RepositoryUrl>
